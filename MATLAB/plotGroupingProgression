xvals = nX;
% xvals = nTot;

Y_med = median(Y(:,11:end), 2);
plot(xvals, Y_med, 'o-')

logx = log10(xvals);
p = polyfit(logx(4:end), Y_med(4:end)', 1);

xvals2 = 1:50;
logx2 = log10(xvals2);

Y2 = polyval(p, logx2);
plot(xvals2, Y2, 'r.-')

